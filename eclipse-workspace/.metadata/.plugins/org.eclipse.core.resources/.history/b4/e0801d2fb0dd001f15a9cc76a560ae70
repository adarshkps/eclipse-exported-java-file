package com.jumpstatement;

public class continue_1 {

	public static void main(String[] args) {
		
		for(int i=0;i<5;i++)
		{
			if (i!=-1)
			{
				continue;
			}
			System.out.println("hello world");
		}
/*		for(int i=0;i<5;i++)
		{
			if (i==3)
			{
				continue;
			}
			System.out.println("hello world");
	*/	}
	}


