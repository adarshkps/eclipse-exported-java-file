package polymorphism;

public class methodOverloading {

	int num1;
	int num2;
	int num3;
	public void add(int a, int b)
	{
		num1=a;
		num2=b;
		System.out.println(num1+num2);
	}
	
	public static void main(String[] args) {
		methodOverloading obj1=new methodOverloading();
		obj1.add(20, 30);
				
	}
}
