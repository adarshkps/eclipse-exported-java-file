package inheritance.AccessSpec1;

public class classA {

	public int pub=10;
	private int pri=20;
	protected int pro=30;
	          int def=40;
	void printData()
	{
		System.out.println(pub);
		System.out.println(pri);
		System.out.println(pro);
		System.out.println(def);
	}
	public static void main(String[] args) {
		
		classA obj=new classA();
	obj.printData();
		 
		obj.pub=100;
		obj.pri=200;
		obj.pro=300;
		obj.def=400;
		 
		
		
		
		
		
	}
}
