package com.encapsulation;

public class EncapsulationforEmp {
public static void main(String[] args) {
	
	Employee e1=new Employee();
	e1.setID(21);
	int i=e1.getId();
	System.out.println(i);
	e1.setName("sharanya");
	String S=e1.getName();
	System.out.println(S);
	e1.setAddress("1-204/21");
	String A=e1.getAddress();
	System.out.println(A);
	e1.setmail("sharanyakp@gmail.com");
	String E=e1.getmail();
	System.out.println(E);
	e1.setSalary(25000.00f);
	float s=e1.getSalary();
	System.out.println(s);
	
	
	
}
}
